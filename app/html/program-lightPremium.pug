include block_html/_style.pug
include block_html/_include.pug


.page#header
	+header()
	.board__el.bg-navy
		.main-cont
			.board__el-wrap.board__el-wrap--extend
				.board__content
					.board__title Light premium <br> 1200 Ккал
					.program-coast.hidden-on-mobile
						.program-coast__el
							.program-coast__title Цена за 6 дней
							.program-coast__val от 6000 руб
						.program-coast__el
							.program-coast__title Цена за прием
							.program-coast__val от 180 руб
					a(href="#program").js-scroll-to.btn--md.btn--white.hidden-on-mobile Заказать
				.board__img(style="background-image:url('img/slide-person-12.png')")
	.program-coast.program-coast--dark.visible-on-mobile
		.program-coast__el
			.program-coast__title Цена за 6 дней
			.program-coast__val от 6000 руб
		.program-coast__el
			.program-coast__title Цена за прием
			.program-coast__val от 180 руб
	.main-cont.visible-on-mobile.mb-24
		a(href="#program").js-scroll-to.btn--md.btn--mark.btn--mobile Выбрать программу
	.content.bg-navyLight
		.section-sm
			.main-cont-sm.text
				.type--lgx.type--head.type--center.mb-24 Худей с легкостью
				.type--md.type--black.type--center Доставка еды для сброса веса. Эти программы идеальны для тех, кто борется с лишним весом. Потребляйте меньше ккал, чем расходуете. Залог успеха — минимум жиров и углеводов, максимум белков и клетчатки!

		// step work
		.section-sm
			.main-cont
				.work-step
					.work-step__el.bg-white
						.work-step__el-icon
							img(src="img/icon/menu-list--navy.svg", alt="")
						.type--md.type--bold.type-navy.mb-12 Разнообразное меню
						.type-navy 600+ блюд, без трансжиров, сахара и консервантов. Новое меню каждую неделю
					.work-step__el.bg-white
						.work-step__el-icon
							img(src="img/icon/stopwatch--navy.svg", alt="")
						.type--md.type--bold.type-navy.mb-12 Экономия времени
						.type-navy Выбирайте удобное время для доставки, избавьтесь от изнурительных походов в магазин и мытья посуды
					.work-step__el.bg-white
						.work-step__el-icon
							img(src="img/icon/fresh--navy.svg", alt="")
						.type--md.type--bold.type-navy.mb-12 Это выгодно
						.type-navy Один приём пищи стоит от 169. Дешевле, чем в кафе и ресторане и быстрее, чем готовка дома
		// step work === end


		// program constructor
		.program-main-wrap#program
			.js-tab-wrap
				// choose program
				.program-group-wrap
					.main-cont
						.program-group.js-tab-cont.js-tick-cont.active(data-programType="weightLoss")
							.program-group__el.program-group__el--navy.js-tick.active
								.program-group__el-name Normal
								.program-group__el-val 900 Ккал
				// choose program === end


			// program list
			.main-cont
				.program-list-wrap
					.program-list-header
						// program days
						.size.js-tick-cont
							.size__el.size__el--navy.js-tick.active 1я неделя
							.size__el.size__el--navy.js-tick 2я неделя
						// program days
						.program-days.js-tick-cont
							-
								var items = ['Пн', 'Вт', 'Ср', 'Чт', 'Пт', 'Сб'];
							each item,i in items
								.program-days__el.program-days__el--navy.js-tick(class=i === 0 ? 'active' : '') #{item}
						// program days === end
						// program consist
						.program-consist
							.program-consist__el
								.program-consist__el-val 2312
								.program-consist__el-type Ккал
							.program-consist__el
								.program-consist__el-val 456
								.program-consist__el-type Белки
							.program-consist__el
								.program-consist__el-val 326,9
								.program-consist__el-type Жиры
							.program-consist__el
								.program-consist__el-val 458
								.program-consist__el-type Углеводы
						// program consist === end
					.program-list-container
						.program-list
							- for (var i=0;i<6;i++)
								+item(2,'change')
							.program-banner.program-banner-inner.br-12
								img(src="img/smile/smile-star.png", alt="").program-banner__img
								.type--md.type--false от 280
								.program-banner__cost 180
								.program-banner__text руб/день
								a(href="#").btn--md.btn--navy Заказать

			// program list === end
		// program constructor === end

		.main-cont
			.program-banner.program-banner--mobile
				.program-banner-inner
					img(src="img/smile/smile-star.png", alt="").program-banner__img
					.program-banner__cost 180
					.program-banner__text руб/день
					a(href="#order").btn--md.btn--navy.js-scroll-to Заказать


		.section-contract.p-80.mb-100.bg-navy.js-stopShrink#order
			.main-cont-sm
				.contract
					.contract-head.bg-navySecond
						.contract-head-info
							.contract-head-title Заказать Light premium
							.contract-advant
								img(src="img/smile/smile-heart-eyes.png", alt="")
								span без мяса
						.contract-head-img(style="background-image:url('img/slide-person-12.png')")
					.contract-content
						.contract-config-wrap.js-contract-config-wrap
							.type--bold.type--md.mb-24 Количество дней
							.g.g-3.sm__g-1.g-gap-24.mb-24
								.contract-config__el.contract-config__el--navy.active.js-contract-el.js-switch(data-group="programRange" data-item="start")
									+radio('5 дней',"range","true")
								.contract-config__el.contract-config__el--navy.js-contract-el.js-switch(data-group="programRange" data-item="6days")
									+radio('6 дней',"range")
								.contract-config__el.contract-config__el--navy.js-contract-el.js-switch(data-group="programRange" data-item="7days")
									+radio('7 дней',"range")
						.contract-config-wrap.js-contract-config-wrap
							.type--bold.type--md.mb-24 Периодичность
							.g.g-2.md__g-2.sm__g-1.g-gap-20.mb-32
								.contract-config__el.contract-config__el--navy.active.js-contract-el.js-switch(data-group="programSize" data-item="start")
									+radio('Пробный день',"list","true")
								.contract-config__el.contract-config__el--navy.js-contract-el.js-switch(data-group="programSize" data-item="6days")
									+radio('Неделя (6 дней)',"list")
								.contract-config__el.contract-config__el--navy.js-contract-el.js-switch(data-group="programSize" data-item="2week")
									+radio('2 недели (12 дней)',"list")
									.type--bold.type--navy -5%
								.contract-config__el.contract-config__el--navy.js-contract-el.js-switch(data-group="programSize" data-item="4week")
									+radio('4 недели (24 дня)',"list")
									.type--bold.type--navy -7.5%

							.icon-row.icon-row--extend.js-switch-cont.hidden(data-group="programSize" data-item="3month")
								+icon("info","icon icon--right icon--orange")
								span.type--sm Подписка оплачивается сразу на 2 недели и еженедельно в последующий срок
						.input-cont
							.input-row
								label.check-el.check-el--navy.js-switch(data-item="isCoupone")
									.style-label.style-label--checkbox
										input(type="checkbox" ).style-input
										.style-input-element
											+icon("checked","icon")
										.style-input-text У меня есть купон
						.input-cont.hidden.js-switch-cont(data-item="isCoupone")
							.input-cont.input-cont--three
								.input-row.input-row--validate
									input(type="text" data-placeholder="Номер купона" class="input")
									.input-msg Купон не подходит
									+icon("close","icon icon-validate-false")
									+icon("checked","icon icon-validate-true")
								.input-row.input-row--validate
									input(type="text" data-placeholder="Телефон" class="input input-mask--phone")
									.input-msg Введённые данные не должны содержать цифры
									+icon("close","icon icon-validate-false")
									+icon("checked","icon icon-validate-true")
								.input-row
									.btn--md.btn--light.btn--mobile.disable Применить
						.input-cont.js-switch-cont(data-item="isCoupone")
							.input-row
								input(type="text" data-placeholder="Телефон" class="input input-mask--phone")
						.contract-total-wrap
							.contract-total
								.contract-total__name Extralight
								.contract-total__val 6300 руб
							.contract-total
								.contract-total__name Итого
								.contract-total__val.type-navy.type--bold.type--md 6300 руб
					.contract-footer
						.contract-footer__text Нажимая кнопку “Оформить” я принимаю условия условия <a href="#" class="type-navySecond">передачи информации</a>
						a(href="#").btn--md.btn--navy.btn--mobile
							.btn-icon-row.btn-icon-row--right
								span Оформить
								+icon("arrow-right","icon icon--left icon--white")
		// contract === end

		// advant
		.main-cont
			.advant.mb-100
				.advant__el
					.f.f-column.f-grow
						.icon--lgx.mb-32
							img(src="img/icon/menu-list--navy.svg", alt="")
						.f.f-column.f-grow
							.type--bold.mb-5 Разнообразное меню
							.type--main.type--primary.sm__hide.mb-16 600+ блюд, без трансжиров, сахара и консервантов
							.type--secondary.mt-a Подробнее
				.advant__el
					.f.f-column.f-grow
						.icon--lgx.mb-32
							img(src="img/icon/schedule--navy.svg", alt="")
						.f.f-column.f-grow
							.type--bold.mb-5 Умная доставка
							.type--main.type--primary.sm__hide.mb-16 Доставляем в часовой интервал утром и 2х-часовой вечером
							.type--secondary.mt-a Подробнее
				.advant__el
					.f.f-column.f-grow
						.icon--lgx.mb-32
							img(src="img/icon/stopwatch--navy.svg", alt="")
						.f.f-column.f-grow
							.type--bold.mb-5 Экономия времени
							.type--main.type--primary.sm__hide.mb-16 Избавьтесь от изнурительного мытья посуды и готовки
							.type--secondary.mt-a Подробнее
				.advant__el
					.f.f-column.f-grow
						.icon--lgx.mb-32
							img(src="img/icon/menu-check--navy.svg", alt="")
						.f.f-column.f-grow
							.type--bold.mb-5 Расчитаны КБЖУ
							.type--main.type--primary.sm__hide.mb-16 Рацион сбалансирован, мы уже посчитали КБЖУ за Вас
							.type--secondary.mt-a Подробнее
		// advant === end

		//review
		.section-extend.bg-navy
			.main-cont.js-slider-wrap
				.review-head
					.review-head__info
						.type--head.type--lgx.type--white.mb-16 Отзывы клиентов
						.type--white Отзывы реальных клиентов о доставке правильного питания на неделю, вкусе самой еды, ее качестве и сервисе компании BeFit. Узнай почему у нас так вкусно!
					.review-control.slider-control-row
						.slider-control.slider-control--light.js-control-left
							+icon("arrow-left","icon")
						.slider-control.slider-control--light.js-control-right
							+icon("arrow-right","icon")

				.review
					.slider-wrap
						.slider-review.slider-extend.js-slider
							- for (var j=1;j<5;j++)
								- for (var i=1;i<5;i++)
									.slider-el-wrap
										.slider-review__el(class=bg ? 'slider-review__el--border' : '')
											.slider-review-head
												.slider-review-head__user(style="background-image:url('img/user.jpg')")
												.slider-review-head__info
													.type--head mike_beyond_the_sea
													.type--sm Moskow
											.slider-review-img(style="background-image:url('img/inst-img-" + i + ".jpg')")
											.slider-review-wrap
												.slider-review-content.scroll-template-light
													.slider-review-text Мы то - что мы едим.Пожалуй, лучшее мое решение за последнее время - это отказ от вредной пищи, сладкого, мучного и бокальчика вина за ужином. (да, черт подери, сама не верю в это, но Алкоголь тоже попал под санкционку в моей жизни)И знаете, это было решение за последнее время  - это отказ от мучного и бокальчика вина за ужином. (да, черт подери, сама не верю в это, но Алкоголь тоже попал под санкционку в моей жизни)И знаете, это было решение за последнее время  - это отказ от

		.main-cont.js-tab-wrap
			.title.title--lgx Часто задаваемые вопросы
			.faq-wrap
				.faq-type.js-tick-cont
					.faq-type__el.faq-type__el--navy.js-tick.js-tab.active
						+icon("apple","icon icon--lg")
						span.type-navy Диета и питание
					.faq-type__el.faq-type__el--navy.js-tick.js-tab
						+icon("money","icon icon--lg")
						span.type-navy Заказ и оплата
					.faq-type__el.faq-type__el--navy.js-tick.js-tab
						+icon("deliver","icon icon--lg")
						span.type-navy Доставка
					.faq-type__el.faq-type__el--navy.js-tick.js-tab
						+icon("cook","icon icon--lg")
						span.type-navy Приготовление

			-
				var items = [
					{
						"list": [
							'1. Как понять что и когда есть?',
							'2. Как часто употреблять рационы?',
							'3. Кто занимается составлением меню?',
							'4. Как часто обновляется меню?',
							'5. Могу ли я заменить или исключить блюдо?'
						]
					},
					{
						"list": [
							'1. Как понять что и когда есть?',
							'2. Как часто обновляется меню?',
							'3. Могу ли я заменить или исключить блюдо?'
						]
					},
					{
						"list": [
							'1. Как понять что и когда есть?',
							'2. Как часто употреблять рационы?',
							'3. Кто занимается составлением меню?',
							'4. Могу ли я заменить или исключить блюдо?'
						]
					},
					{
						"list": [
							'1. Как понять что и когда есть?',
							'2. Как часто употреблять рационы?',
							'3. Кто занимается составлением меню?',
							'4. Как часто обновляется меню?',
							'5. Могу ли я заменить или исключить блюдо?',
							'6. Отзывы реальных клиентов о доставке правильного?'
						]
					}
				];
			each item,i in items
				.accordion.js-accordion.js-tab-cont(class=i === 0 ? 'active' : '')
					each itemList in item.list
						.accordion__el.js-accordion-el
							.accordion__el-head.js-accordion-head
								.accordion__el-head-text #{itemList}
								.accordion__el-head-icon
							.accordion__el-content.text.js-accordion-content
								p Платежный план «переезд в готовый дом» - дает возможность будущему покупателю переехать из квартиры в загородный дом без полной оплаты за домовладение! Для быстрого переезда в готовый загородный дом достаточно внести 10% от стоимости коттеджа и Вы можете заселяться!
								p На продажу имеющейся в собственности у покупателя квартиры предоставляется рассрочка четыре месяца. По факту реализации квартиры оплачивается остаточная стоимость за домовладение.

			// faq === end

	+footer()
include ./block_html/_script.pug
